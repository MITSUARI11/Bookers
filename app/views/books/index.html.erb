<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

<h1>Books</h1>

<%= @books.each do |book| %>
  
  <p>Title</p>
  <span>
    <%= link_to book.title, book_path(book.id) %>
  </span>
  
  
<% end %>

<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <p>Title</p>
  <%= f.text_field :title %>
  
  <p>Body</p>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>

<% end %>




